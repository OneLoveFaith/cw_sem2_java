<?xml version="1.0" encoding="UTF-8"?>

<!--Imports-->
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.pharmacy.Controllers.MedSearch">
    <children>
        <AnchorPane prefHeight="86.0" prefWidth="900.0" style="-fx-background-color: #2e3348;">
            <children>
                <Label layoutX="366.0" layoutY="14.0" prefHeight="60.0" prefWidth="168.0" text="Search a medicine" textAlignment="CENTER" textFill="WHITE">
                    <font>
                        <Font size="20.0" />
                    </font></Label>
<!--                Log out button-->
                <Button fx:id="LogOutButton" layoutX="802.0" layoutY="31.0" mnemonicParsing="false" text="Log out" />
<!--                Home button-->
                <Button fx:id="HomeButton" layoutX="75.0" layoutY="31.0" mnemonicParsing="false" text="Home" />
            </children>
        </AnchorPane>
<!--        Name or serial number field-->
      <TextField fx:id="search" layoutX="301.0" layoutY="120.0" prefHeight="50.0" prefWidth="299.0" promptText="Enter name or serial number">
         <font>
            <Font size="20.0" />
         </font>
      </TextField>
<!--        Name-->
      <Text layoutX="151.0" layoutY="243.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Name: ">
         <font>
            <Font size="40.0" />
         </font>
      </Text>
<!--        Quantity-->
      <Text layoutX="151.0" layoutY="288.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Quantity: ">
         <font>
            <Font size="40.0" />
         </font>
      </Text>
<!--        Price-->
      <Text layoutX="151.0" layoutY="341.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Price: ">
         <font>
            <Font size="40.0" />
         </font>
      </Text>
<!--        Serial number-->
      <Text layoutX="154.0" layoutY="394.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Serial number: ">
         <font>
            <Font size="40.0" />
         </font>
      </Text>
      <Text fx:id="name" layoutX="286.0" layoutY="242.0" strokeType="OUTSIDE" strokeWidth="0.0">
         <font>
            <Font size="40.0" />
         </font>
      </Text>
      <Text fx:id="quant" layoutX="323.0" layoutY="285.0" strokeType="OUTSIDE" strokeWidth="0.0">
         <font>
            <Font size="40.0" />
         </font>
      </Text>
      <Text fx:id="price" layoutX="256.0" layoutY="340.0" strokeType="OUTSIDE" strokeWidth="0.0">
         <font>
            <Font size="40.0" />
         </font>
      </Text>
      <Text fx:id="serialNumber" layoutX="414.0" layoutY="393.0" strokeType="OUTSIDE" strokeWidth="0.0">
         <font>
            <Font size="40.0" />
         </font>
      </Text>
      <Button fx:id="searchButton" layoutX="610.0" layoutY="123.0" mnemonicParsing="false" text="Search">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Text layoutX="152.0" layoutY="447.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Date: ">
         <font>
            <Font size="40.0" />
         </font>
      </Text>
      <Text fx:id="date" layoutX="256.0" layoutY="446.0" strokeType="OUTSIDE" strokeWidth="0.0">
         <font>
            <Font size="40.0" />
         </font>
      </Text>
    </children>
</AnchorPane>
