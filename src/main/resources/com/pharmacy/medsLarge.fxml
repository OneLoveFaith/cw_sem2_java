<?xml version="1.0" encoding="UTF-8"?>

<!--Imports-->
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.pharmacy.Controllers.MedsLarge">
    <children>
        <AnchorPane prefHeight="86.0" prefWidth="900.0" style="-fx-background-color: #2e3348;">
            <children>
                <Label layoutX="306.0" layoutY="14.0" prefHeight="60.0" prefWidth="288.0" text="Medicines with a large amount" textAlignment="CENTER" textFill="WHITE">
                    <font>
                        <Font size="20.0" />
                    </font></Label>
<!--                Log out button-->
                <Button fx:id="LogOutButton" layoutX="802.0" layoutY="31.0" mnemonicParsing="false" text="Log out" />
<!--                Home button-->
                <Button fx:id="HomeButton" layoutX="75.0" layoutY="31.0" mnemonicParsing="false" text="Home" />
            </children>
        </AnchorPane>
<!--        Table of medicines with large quantity-->
      <TableView fx:id="meds" layoutX="306.0" layoutY="151.0" prefHeight="299.0" prefWidth="288.0">
        <columns>
          <TableColumn fx:id="name" prefWidth="146.0" text="Name" />
          <TableColumn fx:id="quant" prefWidth="141.0" text="Quantity" />
        </columns>
      </TableView>
    </children>
</AnchorPane>
